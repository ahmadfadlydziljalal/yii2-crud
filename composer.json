{
    "name": "ahmadfadlydziljalal/yii2-crud",
    "description": "CRUD generator steroid, using dynamic form for master-detail paradigm",
    "type": "yii2-extension",
    "keywords": ["yii2", "gii", "generator", "crud"],
    "license": "MIT",
    "authors": [
        {
            "name": "dzil",
            "email": "ahmadfadlydziljalal@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-gii": "^2.2",
        "yiisoft/yii2-bootstrap5": "^2.0",
        "wbraganca/yii2-dynamicform": "dev-dev-dzil"
    },
    "autoload": {
        "psr-4": {
            "dzil\\crud\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ahmadfadlydziljalal/yii2-dynamicform"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "bootstrap": "dzil\\crud\\Bootstrap"
    }
}
